{
  "name": "nouislider-react",
  "private": false,
  "version": "3.3.3",
  "description": "React component wrapping leongersen/noUiSlider",
  "main": "dist/nouislider-react.cjs.js",
  "module": "dist/nouislider-react.esm.js",
  "browser": "dist/nouislider-react.umd.production.min.js",
  "typings": "index.d.ts",
  "scripts": {
    "clear": "rm -rf dist",
    "build": "npm run clear && NODE_ENV=development rollup -c && NODE_ENV=production rollup -c",
    "test": "jest ./tests/",
    "test:coverage": "jest --coverage ./tests/",
    "coveralls": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "test:watch": "jest --watch ./tests/",
    "prepublishOnly": "npm run build",
    "prettier": "prettier --write src/*.js",
    "precommit": "lint-staged"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/setupJest.js"
    ]
  },
  "lint-staged": {
    "src/*.js": [
      "npm run prettier",
      "git add"
    ],
    "*.md": [
      "npm run prettier",
      "git add"
    ]
  },
  "files": [
    "dist",
    "index.d.ts",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mmarkelov/react-nouislider"
  },
  "keywords": [
    "react",
    "nouislider",
    "slider",
    "component",
    "reactjs",
    "range",
    "slider"
  ],
  "author": "Maksim Markelov <mapkeji26@gmail.com>",
  "homepage": "https://github.com/mmarkelov/react-nouislider",
  "license": "MIT",
  "dependencies": {
    "nouislider": "^14.0.2"
  },
  "peerDependencies": {
    "nouislider": ">= 11.x",
    "react": ">= 16.8.x"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "coveralls": "^3.0.6",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint": "^6.3.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "husky": "^3.0.5",
    "jest": "^24.9.0",
    "lint-staged": "^9.2.5",
    "prettier": "^1.18.2",
    "prop-types": "^15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "rollup": "^1.20.3",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^5.1.1"
  }

